{
  "name": "@norges-domstoler/dds-design-tokens",
  "version": "3.0.2",
  "description": "Design tokens used in Domstolenes designsystem",
  "author": "DDS team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/designsystem"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "homepage": "https://design.domstol.no",
  "keywords": [
    "dds",
    "design",
    "tokens",
    "design tokens",
    "design system",
    "domstolene",
    "norges domstoler",
    "elsa"
  ],
  "scripts": {
    "clean": "rm -r dist dds/build || :",
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "build:clean": "npm run clean && npm run build",
    "build:tokens": "node ./dds/build.js",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/node": "^20.9.2",
    "clean-package": "2.2.0",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "rollup": "^3.29.4",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-sass": "^1.12.21",
    "rollup-plugin-typescript2": "^0.36.0",
    "style-dictionary": "^3.7.2",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "provenance": true
  },
  "clean-package": "./clean-package.config.json"
}
