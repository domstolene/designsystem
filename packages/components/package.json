{
  "name": "@norges-domstoler/dds-components",
  "version": "15.9.1",
  "description": "React components used in Elsa - domstolenes designsystem",
  "author": "Elsa team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/designsystem"
  },
  "sideEffects": [
    "*.css",
    "src/styles/index.ts"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./index.css": "./dist/index.css"
  },
  "homepage": "https://design.domstol.no",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "generate-css-types": "tcm src",
    "build": "pnpm run generate-css-types && tsup --dts",
    "dev": "pnpm run generate-css-types --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "dds",
    "design",
    "tokens",
    "design tokens",
    "design system",
    "domstolene",
    "norges domstoler",
    "elsa"
  ],
  "devDependencies": {
    "@internationalized/date": "^3.5.3",
    "@norges-domstoler/dds-design-tokens": "workspace:*",
    "@norges-domstoler/storybook-components": "workspace:*",
    "@react-types/datepicker": "^3.7.3",
    "@storybook/blocks": "8.1.1",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "csstype": "3.1.2",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "styled-components": "^6.1.11",
    "tsup": "^8.0.2",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "@internationalized/date": "^3",
    "@norges-domstoler/dds-design-tokens": ">=4.0.0",
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "styled-components": "^5 || ^6"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.0",
    "@norges-domstoler/development-utils": "workspace:*",
    "@react-aria/button": "^3.9.4",
    "@react-aria/calendar": "^3.5.7",
    "@react-aria/datepicker": "^3.10.0",
    "@react-aria/focus": "^3.17.0",
    "@react-aria/i18n": "^3.11.0",
    "@react-stately/calendar": "^3.5.0",
    "@react-stately/datepicker": "^3.9.3",
    "file-selector": "^0.6.0",
    "react-select": "^5.8.0"
  },
  "publishConfig": {
    "provenance": true
  }
}
