{
  "name": "@norges-domstoler/development-utils",
  "version": "1.0.1",
  "description": "Development utilities for developing applications in Norges Domstoler",
  "author": "Elsa team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/designsystem"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/development-utils.cjs",
  "module": "./dist/development-utils.js",
  "exports": {
    ".": {
      "import": "./dist/development-utils.js",
      "require": "./dist/development-utils.cjs"
    }
  },
  "homepage": "https://design.domstol.no",
  "scripts": {
    "clean": "rm -r dist || :",
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "build:clean": "npm run clean && npm run build",
    "verify:types": "tsc --noEmit",
    "verify:lint": "npm run lint",
    "verify:prettier": "npm run prettier:check",
    "verify": "concurrently 'npm:verify:*'"
  },
  "keywords": [
    "dds",
    "design",
    "development-utils",
    "design system",
    "domstolene",
    "norges domstoler",
    "elsa"
  ],
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@mdx-js/react": "^2.2.1",
    "@swc/plugin-styled-components": "^1.5.49",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "concurrently": "^7.4.0",
    "esbuild": "^0.16.3",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6",
    "typescript": "^5.0.2",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.1"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "styled-components": "^5"
  }
}
