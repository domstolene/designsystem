{
  "name": "@norges-domstoler/development-utils",
  "version": "1.2.0",
  "description": "Development utilities for developing applications in Norges Domstoler",
  "author": "Elsa team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/designsystem"
  },
  "files": [
    "dist"
  ],
  "main": "src/index.ts",
  "homepage": "https://design.domstol.no",
  "scripts": {
    "build": "tsup --dts",
    "build:fast": "tsup",
    "dev": "npm run build:fast -- --watch",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "verify:types": "tsc --noEmit",
    "verify:lint": "npm run lint",
    "verify:prettier": "npm run prettier:check",
    "verify": "concurrently 'npm:verify:*'",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "keywords": [
    "dds",
    "design",
    "development-utils",
    "design system",
    "domstolene",
    "norges domstoler",
    "elsa"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@mdx-js/react": "^2.3.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^5.62.0",
    "clean-package": "2.2.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "styled-components": "^5 || ^6"
  },
  "publishConfig": {
    "provenance": true
  },
  "clean-package": "../../clean-package.config.json"
}
