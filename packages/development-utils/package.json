{
  "name": "@norges-domstoler/development-utils",
  "version": "1.1.0",
  "description": "Development utilities for developing applications in Norges Domstoler",
  "author": "Elsa team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/designsystem"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/development-utils.cjs",
  "module": "./dist/development-utils.js",
  "exports": {
    ".": {
      "import": "./dist/development-utils.js",
      "require": "./dist/development-utils.cjs"
    }
  },
  "homepage": "https://design.domstol.no",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "verify:types": "tsc --noEmit",
    "verify:lint": "npm run lint",
    "verify:prettier": "npm run prettier:check",
    "verify": "concurrently 'npm:verify:*'"
  },
  "keywords": [
    "dds",
    "design",
    "development-utils",
    "design system",
    "domstolene",
    "norges domstoler",
    "elsa"
  ],
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@mdx-js/react": "^2.3.0",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "concurrently": "^8.2.0",
    "esbuild": "^0.18.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.11",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.32.0"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "styled-components": "^5"
  },
  "publishConfig": {
    "provenance": true
  }
}
