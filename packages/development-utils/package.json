{
  "name": "@norges-domstoler/development-utils",
  "version": "2.0.1",
  "description": "Development utilities for developing applications in Norges Domstoler",
  "author": "Elsa team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/designsystem"
  },
  "sideEffects": [
    "*.css",
    "src/styles/index.ts"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./index.css": "./dist/index.css"
  },
  "homepage": "https://design.domstol.no",
  "scripts": {
    "generate-css-types": "tcm src",
    "build": "pnpm run generate-css-types && tsup --dts",
    "dev": "pnpm run generate-css-types --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "dds",
    "design",
    "development-utils",
    "design system",
    "domstolene",
    "norges domstoler",
    "elsa"
  ],
  "devDependencies": {
    "@norges-domstoler/storybook-components": "workspace:*",
    "@testing-library/react": "^16.3.1",
    "@types/react": "19.2.7",
    "react": "19.2.3",
    "typed-css-modules": "^0.9.1",
    "autoprefixer": "^10.4.23",
    "csstype": "3.2.3",
    "postcss": "^8.5.6",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.15"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18 || ^19"
  },
  "publishConfig": {
    "provenance": true
  }
}
