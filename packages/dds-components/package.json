{
  "name": "@norges-domstoler/dds-components",
  "version": "21.20.0",
  "description": "React components used in Elsa - domstolenes designsystem",
  "author": "Elsa team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/designsystem"
  },
  "sideEffects": [
    "*.css",
    "src/styles/index.ts"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./index.css": "./dist/index.css"
  },
  "homepage": "https://design.domstol.no",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "generate-css-types": "tcm src",
    "build": "pnpm run generate-css-types && tsup --dts",
    "dev": "pnpm run generate-css-types --watch && tsup",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "dds",
    "design",
    "tokens",
    "design tokens",
    "design system",
    "domstolene",
    "norges domstoler",
    "elsa"
  ],
  "devDependencies": {
    "@internationalized/date": "^3.10.1",
    "@norges-domstoler/dds-design-tokens": "workspace:*",
    "@norges-domstoler/storybook-components": "workspace:*",
    "@react-types/datepicker": "^3.13.3",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "^10.4.23",
    "csstype": "3.2.3",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tsup": "^8.5.1",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  },
  "peerDependencies": {
    "@internationalized/date": "^3",
    "@norges-domstoler/dds-design-tokens": ">=5.0.0",
    "react": "^19",
    "react-dom": "^19"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.6",
    "@react-aria/button": "^3.14.3",
    "@react-aria/calendar": "^3.9.3",
    "@react-aria/datepicker": "^3.15.3",
    "@react-aria/focus": "^3.21.3",
    "@react-aria/i18n": "^3.12.14",
    "@react-stately/calendar": "^3.9.1",
    "@react-stately/datepicker": "^3.15.3",
    "file-selector": "^2.1.2",
    "react-select": "^5.10.2"
  },
  "publishConfig": {
    "provenance": true
  }
}
