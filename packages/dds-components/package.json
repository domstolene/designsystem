{
  "name": "@norges-domstoler/dds-components",
  "version": "19.2.0",
  "description": "React components used in Elsa - domstolenes designsystem",
  "author": "Elsa team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/designsystem"
  },
  "sideEffects": [
    "*.css",
    "src/styles/index.ts"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./index.css": "./dist/index.css"
  },
  "homepage": "https://design.domstol.no",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "generate-css-types": "tcm src",
    "build": "pnpm run generate-css-types && tsup --dts",
    "dev": "pnpm run generate-css-types --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "dds",
    "design",
    "tokens",
    "design tokens",
    "design system",
    "domstolene",
    "norges domstoler",
    "elsa"
  ],
  "devDependencies": {
    "@internationalized/date": "^3.7.0",
    "@norges-domstoler/dds-design-tokens": "workspace:*",
    "@norges-domstoler/storybook-components": "workspace:*",
    "@react-types/datepicker": "^3.10.0",
    "@storybook/blocks": "8.5.8",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.20",
    "csstype": "3.1.2",
    "jsdom": "^26.0.0",
    "postcss": "^8.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsup": "^8.3.6",
    "typed-css-modules": "^0.9.1",
    "typescript": "^5.7.3",
    "vite": "^6.1.1",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "@internationalized/date": "^3",
    "@norges-domstoler/dds-design-tokens": ">=5.0.0",
    "react": "^16 || ^17 || ^18 || ^19",
    "react-dom": "^16 || ^17 || ^18 || ^19"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.1.2",
    "@norges-domstoler/development-utils": "workspace:*",
    "@react-aria/button": "^3.11.1",
    "@react-aria/calendar": "^3.7.0",
    "@react-aria/datepicker": "^3.13.0",
    "@react-aria/focus": "^3.19.1",
    "@react-aria/i18n": "^3.12.5",
    "@react-stately/calendar": "^3.7.0",
    "@react-stately/datepicker": "^3.12.0",
    "file-selector": "^2.1.2",
    "react-select": "^5.10.0"
  },
  "publishConfig": {
    "provenance": true
  }
}
