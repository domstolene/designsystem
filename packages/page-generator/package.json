{
  "name": "@norges-domstoler/dds-page-generator",
  "version": "2.2.4",
  "description": "Page Generator used in Elsa - domstolenes designsystem",
  "author": "Elsa team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/designsystem"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/dds-page-generator.cjs",
  "module": "./dist/dds-page-generator.js",
  "exports": {
    ".": {
      "import": "./dist/dds-page-generator.js",
      "require": "./dist/dds-page-generator.cjs"
    }
  },
  "homepage": "https://design.domstol.no",
  "scripts": {
    "clean": "rm -r dist || :",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "build:clean": "npm run clean && npm run build",
    "verify:types": "tsc --noEmit",
    "verify:lint": "npm run lint",
    "verify:prettier": "npm run prettier:check",
    "verify:test": "npm run test",
    "verify": "concurrently 'npm:verify:*'"
  },
  "keywords": [
    "dds",
    "design",
    "page-generator",
    "design system",
    "domstolene",
    "norges domstoler",
    "elsa"
  ],
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@mdx-js/react": "^2.2.1",
    "@norges-domstoler/dds-components": "*",
    "@norges-domstoler/dds-design-tokens": "*",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.11",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.4.0",
    "esbuild": "^0.16.3",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^5.3.6",
    "typescript": "^5.0.2",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.1.0",
    "vitest": "^0.29.1"
  },
  "dependencies": {
    "@norges-domstoler/dds-components": "^12.1.3",
    "@norges-domstoler/dds-design-tokens": "^3.0.1"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "styled-components": "^5"
  },
  "publishConfig": {
    "provenance": true
  }
}
