{
  "name": "@norges-domstoler/storybook-components",
  "private": true,
  "version": "0.0.0",
  "description": "Storybook components for use in the Elsa design system",
  "author": "Elsa team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/designsystem"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/storybook-components.cjs",
  "module": "./dist/storybook-components.js",
  "exports": {
    ".": {
      "import": "./dist/storybook-components.js",
      "require": "./dist/storybook-components.cjs"
    }
  },
  "homepage": "https://design.domstol.no",
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "verify:types": "tsc --noEmit",
    "verify:lint": "npm run lint",
    "verify:prettier": "npm run prettier:check",
    "verify": "concurrently 'npm:verify:*'"
  },
  "devDependencies": {
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "concurrently": "^8.2.0",
    "esbuild": "^0.18.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^3.2.0",
    "vitest": "^0.32.0"
  },
  "peerDependencies": {
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18"
  },
  "dependencies": {
    "@norges-domstoler/dds-design-tokens": "*",
    "@storybook/addon-docs": "^7.0.20",
    "focus-visible": "^5.2.0",
    "styled-components": "^5.3.11"
  }
}
