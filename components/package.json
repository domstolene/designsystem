{
  "name": "@norges-domstoler/dds-components",
  "version": "6.0.0",
  "description": "React components used in Elsa - domstolenes designsystem",
  "author": "Elsa team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domstolene/designsystem"
  },
  "sideEffects": [
    "dist/index.js",
    "*.css"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "homepage": "https://design.domstol.no",
  "scripts": {
    "clean": "rm -r dist || :",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "storybook": "start-storybook -p 6006 -s ./.storybook/images",
    "build-storybook": "build-storybook -o docs",
    "lint": "eslint --ext=jsx,ts,tsx src",
    "prettier": "prettier --write .",
    "prettier:check": "prettier --check .",
    "build:clean": "npm run clean && npm run build",
    "verify:types": "tsc --noEmit",
    "verify:lint": "npm run lint",
    "verify:test": "npm test",
    "verify:prettier": "npm run prettier:check",
    "verify": "concurrently 'npm:verify:*'"
  },
  "keywords": [
    "dds",
    "design",
    "tokens",
    "design tokens",
    "design system",
    "domstolene",
    "norges domstoler",
    "elsa"
  ],
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@mdx-js/react": "^2.1.3",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/addons": "^6.5.10",
    "@storybook/mdx2-csf": "^0.0.3",
    "@storybook/react": "^6.5.10",
    "@storybook/theming": "^6.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^28.1.7",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.3.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-storybook": "^0.6.4",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^29.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.78.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-import-css": "^3.0.3",
    "rollup-plugin-sass": "^1.2.13",
    "rollup-plugin-typescript2": "^0.34.0",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@norges-domstoler/dds-design-tokens": "^1.3.0",
    "react": "^16 || ^17 || ^18",
    "react-dom": "^16 || ^17 || ^18",
    "styled-components": "^5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "moduleNameMapper": {
      "\\.css$": "<rootDir>/__mocks__/styleMock.js"
    },
    "transform": {
      "\\.svg$": "<rootDir>/svgTransform.js"
    }
  },
  "dependencies": {
    "@floating-ui/react-dom": "^1.0.0",
    "focus-visible": "^5.2.0",
    "react-select": "^5.4.0",
    "tslib": "^2.4.0"
  }
}
